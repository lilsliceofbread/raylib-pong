default:
	g++ ../*.cpp -o build.exe -O2 -Wall -Wno-missing-braces -I ../inc/ -L ../lib/ -lraylib -lopengl32 -lgdi32 -lwinmm
debug:
	g++ ../*.cpp -o build.exe -g -Wall -Wno-missing-braces -I ../inc/ -L ../lib/ -lraylib -lopengl32 -lgdi32 -lwinmm
release:
	g++ ../*.cpp -o build.exe -O2 -Wall -Wno-missing-braces -I ../inc/ -L ../lib/ -lraylib -lopengl32 -lgdi32 -lwinmm -mwindows